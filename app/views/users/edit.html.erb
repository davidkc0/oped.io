<h1>Edit profile</h1> 

<%= form_for(@user, html: {mulipart: true}) do |f| %>

<%= f.label :username %> 
<%= f.text_field :username %>

<%= f.label :bio %>
<%= f.text_area :description, placeholder: "Write something about yourself." %>

<%= f.label :url %> 
<%= f.url_field :userurl %>



<%= f.file_field :avatar %>

<%= f.submit "Save changes" %>
<%end%>